{
  "include": [
    "lin_kernighan_tsp_solver"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.*",
    "benchmarks",
    "documentation/archive",
    "htmlcov",
    "images",
    "problems",
    "solutions"
  ],
  "reportMissingImports": true,
